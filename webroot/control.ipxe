#!ipxe

chain --autofree boot.ipxe.cfg ||


:coreos-stable
echo Booting OKD Control Plane
kernel ${okd-kernel-url}
initrd ${okd-initrd-url}
imgargs ${okd-kernel} ip=dhcp rd.neednet=1 initrd=${okd-initrd-url}  console=tty0 console=ttyS0 coreos.inst=yes coreos.inst.install_dev=vda coreos.inst.image_url=${okd-image-url} coreos.live.rootfs_url=${okd-rootfs-url} coreos.inst.ignition_url=${base-url}/${ignition-path}/master.ign
boot




